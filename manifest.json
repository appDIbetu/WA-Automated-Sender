{
    "manifest_version": 2,
    "permissions": [
        "storage"
    ],
    "name": "WA Auto Sender - Send Bulk Messages Free",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "js": [
                "jquery.js",
                "content.js"
            ],
            "matches": [
                "https://web.whatsapp.com/*"
            ]
        }
    ],
    "description": "Send Whatsapp message to a single user or to users in bulk without saving number",
    "icons": {
        "128": "img/icon.png",
        "16": "img/icon.png",
        "48": "img/icon.png"
    },
    "version": "2.3",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html",
        "default_title": "WA Auto Sender | DISup!!"
    }
}